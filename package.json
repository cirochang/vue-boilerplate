{
  "name": "vue-boilerplate",
  "description": "This is a simple boilerplate for Vue",
  "version": "0.0.0",
  "author": "Ciro Chang <cirochang@live.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --disable-host-check --progress --hot --port 8080 --host 0.0.0.0 --config build/webpack.dev.conf.js",
    "build": "webpack --progress --hide-modules --config build/webpack.prod.conf.js",
    "test-unit": "BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test-lint": "eslint ."
  },
  "dependencies": {
