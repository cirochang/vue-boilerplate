{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
   "parserOptions": {
       "parser": "babel-eslint"
   },
  "extends": [
      "airbnb-base",
      "plugin:vue/recommended"
  ],
  "rules": {
    "no-shadow": ["error", { "allow": ["state"] }],
    "no-param-reassign": ["error", { "allow": ["state"] }],
    "import/no-unresolved": "off"
  }
}